	"tokenColors": [
    // Powershell Colors
		{
		  "scope": "comment.block.powershell",
		  "settings": {
			"foreground": "#676B79",
			"fontStyle": "italic"
		  }
    },
    {
      "scope": "comment.line.powershell",
      "settings": {
        "foreground": "#676B79",
        "fontStyle": "italic"
      }
    },
		{
			"scope": "string.quoted.double.powershell",
			"settings": {
				"foreground": "#df81fc"
			}
    },
    {
      "scope": "string.quoted.single.powershell",
      "settings": {
        "foreground": "#df81fc"
      }
    },
		{
			"scope": "variable.other.readwrite.powershell",
			"settings": {
				"foreground": "#96A1FF"
			}
    },
    {
      "scope": "variable.other.member.powershell",
      "settings": {
        "foreground": "#ADDB67"
      }
    },
		{
			"scope": "keyword.control.powershell",
			"settings": {
				"foreground": "#FF75B5"
			}
		},
		{
			"scope": "support.function.attribute.powershell",
			"settings": {
				"foreground": "#46BDFF"
			}
		},
		{
			"scope": "support.function.powershell",
			"settings": {
				"foreground": "#6FC1FF"
			}
		},
		{
			"scope": "keyword.operator.documentation.powershell",
			"settings": {
				"foreground": "#7FDBCA"
			}
		},
		{
			"scope": "entity.name.function.powershell",
			"settings": {
				"foreground": "#46BDFF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "storage.type.powershell",
			"settings": {
				"foreground": "#85EEA7"
			}
    },
    {
      "scope": "meta.scriptblock.powershell",
      "settings": {
          "foreground": "#FF16B0"
        }
    },
    {
      "scope": "source.powershell",
      "settings": {
        "foreground": "#FF16B0"
      }
    },

    // End of Powershell Colors

    // JSON Colors
    {
      "name": "JavaScript Classes",
      "scope": "meta.class entity.name.type.class.js",
      "settings": {
        "foreground": "#ffcb8b"
      }
    },
    {
      "name": "JavaScript Method Declaration e.g. `constructor`",
      "scope": "meta.method.declaration storage.type.js",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "JavaScript Terminator",
      "scope": "terminator.js",
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "JavaScript Meta Punctuation Definition",
      "scope": "meta.js punctuation.definition.js",
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "Entity Names in Code Documentations",
      "scope": [
        "entity.name.type.instance.jsdoc",
        "entity.name.type.instance.phpdoc"
      ],
      "settings": {
        "foreground": "#5f7e97"
      }
    },
    {
      "name": "Other Variables in Code Documentations",
      "scope": ["variable.other.jsdoc", "variable.other.phpdoc"],
      "settings": {
        "foreground": "#78ccf0"
      }
    },
    {
      "name": "JavaScript module imports and exports",
      "scope": [
        "variable.other.meta.import.js",
        "meta.import.js variable.other",
        "variable.other.meta.export.js",
        "meta.export.js variable.other"
      ],
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "JavaScript Variable Parameter Function",
      "scope": "variable.parameter.function.js",
      "settings": {
        "foreground": "#7986E7"
      }
    },
    {
      "name": "JavaScript[React] Variable Other Object",
      "scope": [
        "variable.other.object.js",
        "variable.other.object.jsx",
        "variable.object.property.js",
        "variable.object.property.jsx"
      ],
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "JavaScript Variables",
      "scope": ["variable.js", "variable.other.js"],
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "JavaScript Entity Name Type",
      "scope": ["entity.name.type.js", "entity.name.type.module.js"],
      "settings": {
        "foreground": "#ffcb8b",
        "fontStyle": ""
      }
    },
    {
      "name": "JavaScript Support Classes",
      "scope": "support.class.js",
      "settings": {
        "foreground": "#d6deeb"
      }
    },
    {
      "name": "JSON Property Names",
